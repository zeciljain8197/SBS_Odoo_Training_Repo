<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="view_overall_report_wiz_form" model="ir.ui.view">
        <field name="name">view.overall.report.wiz.form</field>
        <field name="model">overall.report.wizard</field>
        <field name="inherit_id" ref="formula1.view_overall_report_wiz_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='wins']" position="after">
                <field name="entry_fees"/>
                <field name="c_code"/>
            </xpath>
        </field>
    </record>
</odoo>